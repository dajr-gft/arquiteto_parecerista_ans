# Google Cloud Configuration
GOOGLE_GENAI_USE_VERTEXAI=True
GOOGLE_CLOUD_PROJECT=gft-bu-gcp
GOOGLE_CLOUD_LOCATION=us-central1

# Agent Configuration
AGENT_NAME=ans_expert_agent
AGENT_MODEL=gemini-2.5-pro
AGENT_DESCRIPTION=Business and Solutions Architecture Agent - Expert in ANS domain for Banco BV
APP_VERSION=1.0.0

# Security Configuration
MAX_FILE_SIZE=10485760  # 10 MB in bytes
MAX_FILES=5

# Logging Configuration
LOG_LEVEL=INFO

# ============================================================================
# AUTHENTICATION
# ============================================================================
# Este agente usa AUTENTICAÇÃO DO GCP (IAM) via Service Account.
# Configurado em: contract.yml (serviceAccountName: ans-agent-sa)
#
# Não é necessário configurar API Keys - a autenticação é feita
# automaticamente pelo Vertex AI usando o Service Account do GKE.
#
# Se precisar de autenticação adicional via API Keys no futuro,
# veja: src/utils/examples/auth_example.py
# ============================================================================

