# Google Cloud Configuration
# Set to True to use Vertex AI, False to use Google AI Studio
GOOGLE_GENAI_USE_VERTEXAI=True

# Your Google Cloud Project ID
GOOGLE_CLOUD_PROJECT=your-project-id

# Google Cloud Location/Region
GOOGLE_CLOUD_LOCATION=us-central1

# Agent Configuration
# Name of the agent
AGENT_NAME=ans_expert_agent

# Model to use (e.g., gemini-2.5-pro, gemini-1.5-pro)
AGENT_MODEL=gemini-2.5-pro

# Agent description
AGENT_DESCRIPTION=Business and Solutions Architecture Agent - Expert in ANS domain for Banco BV

